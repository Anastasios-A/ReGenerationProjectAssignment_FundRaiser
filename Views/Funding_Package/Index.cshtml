@model IEnumerable<ReGenerationProjectAssignment_FundRaiser.Models.Funding_Package>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PackageName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PackageValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PackageReward)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PackageName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PackageValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PackageReward)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PackageId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PackageId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PackageId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
